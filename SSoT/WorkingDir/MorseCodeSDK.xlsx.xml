    <RelativeLength>5</RelativeLength>
    <Symbol>_</Symbol>
    <DisplayText>---</DisplayText>
    <Binary>1</Binary>
  </Signal>
  <Signal>
    <Name>ICG</Name>
    <Description>intra-character gap (standard gap between the dots and dashes in a character)</Description>
    <RelativeLength>1</RelativeLength>
    <Symbol>x</Symbol>
    <Binary>0</Binary>
  </Signal>
  <Signal>
    <Name>LongICG</Name>
    <Description>long intra-character gap (longer internal gap used in C, O, R, Y, Z and &amp;)</Description>
    <RelativeLength>2</RelativeLength>
    <Symbol>X</Symbol>
    <Binary>0</Binary>
  </Signal>
  <Signal>
    <Name>ShortGap</Name>
    <Description>short gap (between letters)</Description>
    <RelativeLength>2</RelativeLength>
    <Symbol>l</Symbol>
    <Binary>0</Binary>
  </Signal>
  <Signal>
    <Name>MediumGap</Name>
    <Description>medium gap (between words)</Description>
    <RelativeLength>3</RelativeLength>
    <Symbol>w</Symbol>
    <Binary>0</Binary>
  </Signal>
  <Signal>
    <Name>LongGap</Name>
    <Description>long gap (between sentences)</Description>
    <RelativeLength>4</RelativeLength>
    <Symbol>s</Symbol>
    <Binary>0</Binary>
  </Signal>
